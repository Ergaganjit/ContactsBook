<table>

    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email id</th>
        <th>Address</th>
        <th>DOB</th>
        <th>Phone</th>

    </tr>

    {{#each pcontacts}}
    <tr>
        <td>{{this.fname}}</td>
        <td>{{this.lname}}</td>
        <td>{{this.email}}</td>
        <td>{{this.address}}</td>
        <td>{{this.dob}}</td>
        <td>{{this.phone}}</td>

    </tr>
    {{/each}}
</table>